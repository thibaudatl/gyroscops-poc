{
    "name": "php-etl/workflow-console-runtime",
    "description": "This library allows you to use the workflow console runtime",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "symfony/console": "^6.0",
        "php-etl/pipeline-contracts": "0.4.*",
        "php-etl/satellite-contracts": "0.1.*",
        "php-etl/pipeline": "*",
        "php-etl/console-state": "*",
        "php-etl/pipeline-console-runtime": "*",
        "php-etl/action-console-runtime": "*",
        "php-etl/action": "*"
    },
    "require-dev": {
        "rector/rector": "^0.15",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Runtime\\Workflow\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.4.x-dev"
        }
    }
}
