{
    "name": "php-etl/fast-map-config",
    "description": "This library implements the Extract-Transform-Load pattern asynchronously in PHP with the help of iterators and generators",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "symfony/expression-language": "^6.0",
        "symfony/property-access": "^6.0",
        "php-etl/metadata": "*",
        "php-etl/mapping-contracts": "0.4.*",
        "php-etl/fast-map": "*"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\FastMapConfig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Component\\FastMapConfig\\": "tests/functional/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.5.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^0.15",
        "phpunit/php-invoker": "*"
    }
}
